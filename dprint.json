{
  "newLineKind": "system",
  "typescript": {
    "semiColons": "asi",
    "quoteStyle": "preferSingle",
    "quoteProps": "asNeeded"
  },
  "json": {
  },
  "markdown": {
    "textWrap": "always"
  },
  "dockerfile": {
  },
  "prettier": {
    "bracketSpacing": true,
    "semi": false,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "includes": ["*.{ts,tsx,mts,js,jsx,cjs,mjs,json,md,dockerfile,html,css,sass,scss,yml,yaml}"],
  "excludes": [
    "dist/",
    "e2e/output/",
    "node_modules/",
    "*-lock.json"
  ],
  "plugins": [
    "https://plugins.dprint.dev/typescript-0.85.1.wasm",
    "https://plugins.dprint.dev/json-0.17.4.wasm",
    "https://plugins.dprint.dev/markdown-0.15.3.wasm",
    "https://plugins.dprint.dev/dockerfile-0.3.0.wasm",
    "https://plugins.dprint.dev/prettier-0.26.1.json@fdbe31f6aecd24f9d6b924214710a6766050d03146163b4e241e6056b2462f2e"
  ]
}
