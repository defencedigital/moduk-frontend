  <ul class="govuk-list govuk-list--bullet">
  {% for exampleName in exampleNames %}
    <li>
      <a class="govuk-link" href="/react/components/{{ component }}/{{ exampleName }}">
        {{ exampleName | replace("-", " ") }}
      </a>
    </li>
  {% endfor %}
  </ul>
  {% endfor %}
</body>
</html>
